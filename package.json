{
  "name": "miso-cursor-windsurf-trial-resetter",
  "productName": "MISO_Cursor_WindSurf Trial Resetter",
  "version": "1.0.0",
  "description": "An application to reset trial periods of software by cleaning leftover files and registry entries.",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "vite build && electron-builder --mac",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "electron",
    "codeium",
    "trial",
    "reset"
  ],
  "author": "MISO",
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "glob": "^11.0.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.15.5",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.1",
    "lucide-react": "^0.516.0",
    "pe-toolkit": "github:ryanc16/pe-toolkit",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "shell-quote": "^1.8.3",
    "sudo-prompt": "^9.2.1",
    "vs-version-info": "github:ryanc16/pe-toolkit",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zustand": "^5.0.5"
  },
  "build": {
    "appId": "com.miso.trialresetter",
    "productName": "MISO_Cursor_WindSurf Trial Resetter",
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "icon": "build/icon.png"
    }
  }
}
